<table class="mdl-cell mdl-cell--12-col mdl-data-table mdl-js-data-table">
    <tbody>
        <tr><th>Strings</th><td>{{#each raw}}<pre>{{#if this}}{{this}}{{else}}&lt;BLANK&gt;{{/if}}</pre>{{/each}}</td></tr>
        <tr><th>Effective Record</th><td><pre>{{#each raw}}{{this}}{{/each}}</pre></td></tr>
        <tr><th>Parsed Record</th>
        <td>
            {{#if record}}
                <div>
                    {{#if parsed}}
                        {{#with parsed}}
                            <span>v=spf1 </span>
                            {{#each this}}
                                <span class="token-{{type.token}}">
                    {{#if rule}}<span class="prefix nospace">{{rule.prefix}}</span>{{else}}<span class="ghost prefix nospace">+</span>{{/if}}<span class="name nospace">{{type.token}}</span>{{#if payload}}<span name="payload nospace">:{{payload}}</span>{{/if}}
                </span>
                            {{/each}}
                        {{/with}}
                    {{else}}
                        Record is invalid
                    {{/if}}
                </div>
            {{else}}
                <div>Invalid record</div>
            {{/if}}
        </td></tr>
    </tbody>
</table>